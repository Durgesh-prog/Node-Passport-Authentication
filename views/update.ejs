<div class='wrapper'>
<h1 style="margin-top:2rem">Update Task</h1>
  <form class='wrapper' action="/user/dashboard/update/<%= id %>" method="POST">
	<div class="form-group" style="margin-top:2rem">
	  <label for="task">Update Task</label>
	  <input
		type="text"
		id="task"
		name="task"
		placeholder="Enter Task"
		value="<%= typeof task != 'undefined' ? email : '' %>"
	  />
	</div>
	<button type="submit" class="btn btn-success" >
	 Update
	</button>
  </form>
</div>

